{
    "manifest_version": 3,
    "name": "Link Connect",
    "description": "Chrome extension to see your linkedin updates and alerts",
    "version": "0.1.0",
    "author": "Greg Babbert",
    "background": {
        "service_worker": "script.js"
    },
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "128": "/images/linkconnect.png"
        }
    },
    "icons": {
        "128": "/images/linkconnect.png"
    }
}